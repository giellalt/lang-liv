!! # Prounoun inflection
! ----------------------
!! This file documents `affixes/pronouns.lexc`,
!! the file on Livonian pronoun  inflection

LEXICON PRON_
!! **LEXICON @LEXNAME@ = goes to # only.
:	#	;

!LEXICON PronCases
!+Pron:   CASES  ;
!        -    Nouns  ;

LEXICON PRON_MIS !!= @CODE@ mis:mi 1
+Interr:ss SG-DAT_õn ;
+Interr:ss SG-ELA_õst ;
+Interr:s SG-GEN ;
+Interr:ss SG-ILL_õ ;
+Interr:ss SG-INE_õs ;
+Interr:ss SG-INS_õks ;
+Interr:s SG-NOM ;
+Interr:s SG-PAR_ZERO ;
+Interr: SG-PAR_dā ;


LEXICON PRON_JEGĀ !!= @CODE@ jegā:jeʼgā 2
:%^StodRM%^CC2C SG-DAT_n ;
:%^StodRM%^CC2C SG-ELA/INE_st ;
:%^StodRM%^CC2C SG-GEN ;
:%^VowsRM SG-ILL_õ ;
:%^StodRM%^CC2C SG-INS_ks ;
:%^StodRM%^CC2C SG-NOM ;
:%^StodRM%^CC2C SG-PAR_ZERO ;


LEXICON PRON_MŪ !!= @CODE@ mū:m 3
:ū SG-DAT_n ;
:ū SG-ELA/INE_stõ ;
:ū SG-GEN ;
:ū SG-ILL_ʼzõ ;
:ū SG-INS_kõks ;
:ū SG-NOM ;
:ū SG-PAR_dõ ;
:ū PL-ELA/INE_šti   ;
:unt PL-INS_ZEROKÕKS-STEM ;
:ū PL-ILL_ʼži ;
:ū PL-PAR_ḑi ;

LEXICON PRON_SE !!= @CODE@ se:s 4
+Dem:īe SG-DAT_n ;
+Dem:īe SG-ELA/INE_stõ ;
+Dem:īe SG-GEN ;
+Dem+Sg+Ill:īʼezõ K ;
+Dem:īe SG-INS_kõks ;
+Dem:e SG-NOM ;
+Dem:ie SG-PAR_dā ;

LEXICON PRON_TÄMĀ !!= @CODE@ tämā: 5
+Pers:täʼm%{XC%} SG-DAT_õn ;
+Pers:täʼm SG-ELA/INE_stõ ;
+Pers:täʼm SG-GEN ;
+Pers:täʼm%{XC%} SG-ILL_õ ;
+Pers:täʼm%{XC%} SG-ILL_õz ;
+Pers:täʼm SG-INS_kõks ;
+Pers:tämā SG-NOM ;
+Pers:ta SG-NOM ;
+Pers:tǟn SG-PAR_da ;
PRON_NE ;

LEXICON PRON_NE !!= @CODE@ ne:n 4 & 5
+Pers+Pl+Dat:näntõn K ;
+Pers:nē PL-ELA/INE_šti  ;
+Pers+Pl+Gen:nänt K ;
+Pers:nē PL-ILL_ʼži ;
+Pers+Pl+Ins:näntkõks K ;
+Pers+Pl+Nom:ne K ;
+Pers+Pl+Nom:nämād K ;
+Pers:nē PL-PAR_ḑi  ;

LEXICON PRON_MINĀ !!= @CODE@ 6 ma:m
+Pers+Sg1+Dat:iʼnnõn K ;
+Pers+Sg1+Ela:iʼnstõ K ;
+Pers+Sg1+Gen:iʼn K ;
+Pers+Sg1+Ill:iʼnnõ K ;
+Pers+Sg1+Ill:iʼnnõz K ;
+Pers+Sg1+Ine:iʼnsõ K ;
+Pers+Sg1+Ins:iʼnkõks K ;
+Pers+Sg1+Nom:inā K ;
+Pers+Sg1+Nom:a K ;
+Pers+Sg1+Par:īnda K ;
 PRON_MĒG ;

LEXICON PRON_MĒG !!= @CODE@ minā:m 6
