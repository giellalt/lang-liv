!! Prounoun inflection
!! ----------------------
!! Livonian pronouns inflect in the same cases as regular
!! nouns.

LEXICON PRON_
+Pron:	#	;

LEXICON PronCases
+Pron:   CASES  ;
!        -    Nouns  ;

LEXICON PRON_MIS !!= @CODE@ mis:mi 1
+Pron+Interr:ss SG-DAT_õn ;
+Pron+Interr:s%^ConsL SG-ELA_õst ;
+Pron+Interr:s SG-GEN ;
+Pron+Interr:s%^ConsL SG-ILL_õ ;
+Pron+Interr:s%^ConsL SG-INS_õks ;
+Pron+Interr:s SG-NOM ;
+Pron+Interr:s SG-PAR_ZERO ;
+Pron+Interr: SG-PAR_dā ;


LEXICON PRON_JEGĀ !!= @CODE@ jegā:jeʼgā 2
+Pron:%^StodRM SG-DAT_n ;
+Pron:%^StodRM SG-ELA_st ;
+Pron:%^StodRM SG-GEN ;
+Pron:%^ConsL SG-ILL_õ ;
+Pron:%^StodRM SG-INS_ks ;
+Pron:%^StodRM SG-NOM ;
+Pron:%^StodRM SG-PAR_ZERO ;


LEXICON PRON_MŪ !!= @CODE@ mū:m 3
+Pron:ū SG-DAT_n ;
+Pron:ū SG-ELA_stõ ;
+Pron:ū SG-GEN ;
+Pron:ū SG-ILL_ʼzõ ;
+Pron:ū SG-INS_kõks ;
+Pron:ū SG-NOM ;
+Pron:ū SG-PAR_dõ ;
+Pron:ū PL-ELA_šti   ;
+Pron+Pl+Gen:unt K ;
+Pron+Pl+Ill:ū PL-ILL_ʼži   ;
+Pron+Pl+Ins:untkõks K ;
+Pron+Pl+Nom:unt K ;
+Pron+Pl+Par:ūḑi K ;

LEXICON PRON_SE !!= @CODE@ se:s 4
+Pron+Dem:īe SG-DAT_n ;
+Pron+Dem:īe SG-ELA_stõ ;
+Pron+Dem:īe SG-GEN ;
+Pron+Dem+Sg+Ill:īʼezõ K ;
+Pron+Dem:īe SG-INS_kõks ;
+Pron+Dem:e SG-NOM ;
+Pron+Dem:ie SG-PAR_dā ;

LEXICON PRON_TÄMĀ !!= @CODE@ tämā:t 5
+Pron+Dem:äʼm%^ConsL SG-DAT_õn ;
